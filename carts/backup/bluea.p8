	if (self.multiball) spr(ic+1,sx+8,5,1,1)  
	if (self.inkshot) spr(ic+2,sx+16,5,1,1)  
	if (self.fireshot) spr(ic+3,sx+24,5,1,1)  
	if (self.glue) spr(ic+4,sx+32,5,1,1)  
