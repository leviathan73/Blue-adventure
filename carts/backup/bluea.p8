--#reg aiming
aiming = {
	ax = 0,
	ay = 0,
	on = false,
	ylim = 80,
	active = false
}

function aiming:reset()
	self.ax = 0
	self.ay = 0
	self.on = false
end

function aiming:place(px,py)
	if self.active then
		self.ax = px
		self.ay = py
		self.on = true
	end
end

function aiming:logic()
	if self.active and btnp(4) then 
		if ball.y<self.ylim then
			if not self.on then
				self:place(ball.x,ball.y)
				sfx(5)
			else
				self:reset()
				sfx(5)
			end
		else
			sfx(4)
		end
	end
end

function aiming:draw(px,py)
	if self.active then
		if(self.on) spr(7,self.ax,self.ay)
	end
end
