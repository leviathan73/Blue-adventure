--#reg multiball
mballs = {
	limit = 2,
	active = true,
	count = 10
}
mball = {
	sprn = 4,
	x=0,
	y=0,
	mx=0,
	my=0,
	cmx=0,
	cmy=0,
	dx=1,
	dy=-1,
	size = 5,
	active=true,
	bcol = false,
	cside = 0
}

function mballs:reset()
	foreach(self, function(b)
		del(self,b)
	end)
	self.count = 10
end

function mballs:start()
	self.active = true
end

function mballs:stop()
	self.active = false
end

function mballs:addball(px,py,pdx,pdy,sn)
	if(not self.active) return false
	if #self <= self.limit then
		local lball = clone(ball)
		lball.x=px
		lball.y=py-5
		lball.dx = pdx/1
		lball.dy = pdy/1
		lball.sprn = sn
		add(self,lball)
	end
end

function mballs:logic()
	if(not self.active) return false
	foreach(self, function(b)
		b:move()
		board:collision(b)
		if b.bcol then 
			if b.cside==1  then 
				b.dy*=-1
			end
			if b.cside==2 then 
				b.dx*=-1
			end
			if b.cside==3 then
				b.dy*=-1
			end
			if b.cside==4 then 
				b.dx*=-1
			end
			b.cside = 0
			b.bcol = false
			board:removetile(b.cmx,b.cmy)
		end
	end)
end

function mballs:draw()
	if(self.active) then
 		foreach(self, function(b)
	 		spr(b.sprn,b.x,b.y,1,1)
		end)
	end
end
