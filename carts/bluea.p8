intro = 0
start = 1
game = 2
gend = 3
credits = 4

function _init()
	stage.goto(intro)
end
function _update60()
	if(stage:is(intro)) intro:update()
	if(stage:is(start)) _update_startscreen()
	if(stage:is(game)) game:update()
	if(stage:is(gend)) _update_endgame()
	if(stage:is(credits)) _update_credits()
end
function _draw()
 	if(stage:is(intro)) intro:draw()
	if(stage:is(start)) _draw_startscreen()
	if(stage:is(game)) _draw_game()
	if(stage:is(gend)) _draw_endgame()
	if(stage:is(credits)) _update_credits()
end

stage = {
	active=1
}
function stage:goto(stg)
	self.stage = stg
	if(self.stage==2) game:init(0)
	if(self.stage==0) intro:init()
end
function stage:is(stg)
	retuen strg==self.activated
end

