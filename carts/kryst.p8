pico-8 cartridge // http://www.pico-8.com
version 16
__lua__

function _init()
	-- pozycja spr x
	x=24
	--pozycja spr y
	y=32
end

function _update() --1,1

	if btnp(❎) then
 	sfx(2)
	end

	if btnp(➡️) then
 	sfx(1)
		x+=8 
	end
	
	if btnp(⬅️) then
		x=x-8 
	end

	if btnp(⬇️) then
		y=y+8 
	end
	
	if btnp(⬆️) then
		y=y-8 
	end

end 

function _draw()
cls()
color(13)
if (y>100) then camera(0,y-100) end
map(0,0)
spr(1,x,y)
print(x)
print(y)
 spr(4,64,64)
end

